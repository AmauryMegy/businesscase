{{ form_start(form) }}
<div class="row">
    <div class="col-lg-4">
        {{ form_row(form.email) }}
    </div>
    <div class="col-lg-4">
        {{ form_row(form.password) }}
    </div>
    <div class="col-lg-4">
        {{ form_row(form.roles) }}
    </div>
    <div class="col-lg-4">
        {{ form_row(form.firstName) }}
    </div>
    <div class="col-lg-4">
        {{ form_row(form.lastName) }}
    </div>
    <div class="col-lg-4">
        {{ form_row(form.username) }}
    </div>
    <div class="col-lg-4">
        {{ form_row(form.phoneNumber) }}
    </div>
    <div class="col-lg-4">
        {{ form_row(form.birthAt) }}
    </div>
    <div class="col-lg-4">
        {{ form_row(form.registeredAt) }}
    </div>
</div>
{{ form_end(form) }}
